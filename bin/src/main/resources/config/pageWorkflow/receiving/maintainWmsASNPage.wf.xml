                <forwards/> 
            </tableCommit>
            
            
            <tablePopup id="printWmsASN" title="printWmsASN" process="wmsTclASNProcess.printWmsASN" enableType="single" containId="true" pageId="printWmsASNPage">
                <enableExpression/>
