<?xml version="1.0" encoding="UTF-8"?>
<RFFormPage id="tclOrderItemCheckRFPage2" title="tclOrderItemCheckRFPage2">
	<RFDisplay id="asnDisplay" load="tclRfOrderManager.orderInfo">
		<!-- format节点：控制页面展示域展示哪些字段及其展示格式 -->
		<format>
		<![CDATA[
			生产工单: {orderCode} 
		]]>
		</format>
		<!-- help节点：罗列页面展示域可供展示的字段列表 -->
		<help>
			生产工单: {orderCode} 
		</help>
	</RFDisplay>
	<RFForm>
		<inputs>
			<RFText id="itemInput" title="itemInput" tip="itemInput" onFoucs="true">
			</RFText>
		</inputs>
		<buttons>
			<RFButton id="itemScanBt" title="itemScanBt" action="tclRfOrderManager.orderItemCommit" validate="" onFoucs="false"/>
		</buttons>
	</RFForm>
</RFFormPage>