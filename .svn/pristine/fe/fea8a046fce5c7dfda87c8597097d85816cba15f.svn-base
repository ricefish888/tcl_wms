        <workflow/>
        <datasource><![CDATA[SELECT 
wslt.id,
wslt.location.code,
wslt.company.name,
wslt.warehouse.name,
wslt.item.code,
wslt.item.name,
wslt.packageUnit.unit,
wslt.billType,
