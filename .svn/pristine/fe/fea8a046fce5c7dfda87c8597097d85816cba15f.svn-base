wslt.subRelateId,
wslt.lineNo,
wslt.qty,
wslt.inventoryStatus,
wslt.lotInfo.storageDate,
wslt.lotInfo.productDate,
wslt.lotInfo.expireDate,
wslt.lotInfo.qaDate,
wslt.lotInfo.lot,
wslt.lotInfo.supplierCode,
wslt.lotInfo.extendPropC9,
