SELECT 
        LOCATION.ID AS "库位.库位ID",
        LOCATION.CODE AS "库位.库位编码",
        AREA.CODE AS "库位.库区编码",
        LOCATION.SEGMENT_NO AS "库位.块",
        LOCATION.LINE_NO AS "库位.排",
        LOCATION.COLUMN_NO AS "库位.列",
        LOCATION.LAYER_NO AS "库位.层",
        LOCATION.PUTAWAY_SEQUENCE AS "库位.上架动线号",
        LOCATION.USE_RATE AS "库位.库满度",
        LOCATION.MIX_RULE AS "库位.混放类型",
        CAPACITY.PALLET_QTY AS "库位.承载托数",
        CAPACITY.QTY_LIMIT AS "库位.承载数量",
        CAPACITY.WEIGHT_LIMIT AS "库位.承载重量",
        CAPACITY.VOLUME_LIMIT AS "库位.承载体积",
        INV.ID AS "库位.库存.库存ID",
        ITEM.ID AS "库位.库存.货品ID",
        ITEMKEY.ID AS "库位.库存.批次属性ID",
        ITEMKEY.LOT AS "库位.库存.批次号",
        ITEMKEY.PRODUCT_DATE AS "库位.库存.生产日期",
        ITEMKEY.SUPPLIER_CODE AS "库位.库存.供应商",
        INV.QTY AS "库位.库存.库存数量"
 FROM WMS_LOCATION LOCATION
 LEFT JOIN WMS_ZONE AREA ON AREA.ID = LOCATION.ZONE_ID
 LEFT JOIN WMS_LOCATION_CAPACITY CAPACITY ON CAPACITY.ID = LOCATION.LOCATION_CAPACITY_ID
 LEFT JOIN WMS_INVENTORY INV ON INV.LOCATION_ID = LOCATION.ID AND INV.QTY > 0
 LEFT JOIN WMS_ITEM_KEY ITEMKEY ON ITEMKEY.ID = INV.ITEM_KEY_ID
 LEFT JOIN WMS_ITEM ITEM ON ITEM.ID = ITEMKEY.ITEM_ID
 WHERE 1=1
 AND LOCATION.INOUT_LOCK in ('OUTLOCKED','UNLOCKED') 
 AND LOCATION.TYPE = 'STORAGE'
 AND LOCATION.STATUS = 'ENABLED'
 AND LOCATION.EXCEPTION_FLAG = 'N'
 AND LOCATION.COUNT_LOCK = 'N'
 AND LOCATION.USE_RATE < 100 
 /~仓库ID: AND LOCATION.WAREHOUSE_ID = {仓库ID}~/
 /~上架库位分类: AND LOCATION.PUTAWAY_CATEGORY = {上架库位分类}~/
 LIMIT 0, 200