<?xml version="1.0" encoding="UTF-8"?>

<page id="importProductionOrderPage">
    <main>
        <process id="productionOrderProcess">
             
              <formCommit id="import" title="import" process="productionOrderProcess.save" enableType="none" multiMapping="false" download="false" closeTransactional="false" visibleType="new">
                <enableExpression/>
                <mappings>
                    <mapping id="import.file.name" className="file"/>
                </mappings>
                <actions>
                    <action managerName="wmsDeliveryOrderManager" methodName="importProductFile" parameter="import.file.name"/>
                </actions>
                <forwards>
                    <forward name="refreshParent" newEnabled="true" editEnabled="true"/>
                </forwards>
            </formCommit>
        
        </process>
    </main>
    <detail/>
</page>




