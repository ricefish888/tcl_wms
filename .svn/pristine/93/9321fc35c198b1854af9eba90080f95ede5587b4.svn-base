package com.vtradex.wms.webservice.sap.model;

import com.vtradex.wms.server.service.annotation.XStreamCDATA;
import com.vtradex.wms.server.service.annotation.XmlDataNote;

/** SAP接口item对象 */
@XmlDataNote("SapItem")
public class SapItem  {

	private String LINENO; //CHAR(32) 行号
	private String MATNR; // CHAR(18) 物料号
	private String MTART; // CHAR(04) 物料类型
	
	@XStreamCDATA   //有特殊字符  需要CDATA
	private String MAKTX; // CHAR(40) 物料描述
	private String MEINS; // CHAR(03) 基本计量单位
	
	@XStreamCDATA   //有特殊字符  需要CDATA
	private String MSEHT; // CHAR(10) 单位描述
	private String MATKL; // CHAR(09) 物料组
	
	@XStreamCDATA   //有特殊字符  需要CDATA
	private String WGBEZ; // CHAR(20) 物料组描述
	private String LVORM; // CHAR(01) 集团删除标识
	private String WERKS; // CHAR(01) 工厂
	private String EKGRP; // CHAR(03) 采购组
	
	@XStreamCDATA   //有特殊字符  需要CDATA
	private String EKNAM; // CHAR(18) 采购组描述
	private String MMSTA; // CHAR(02) 跨工厂物料状态
	private String LVORM1; // CHAR(01) 工厂删除标识  增加了一个1
	
	public String getLINENO() {
		return LINENO;
	}
	public void setLINENO(String lINENO) {
		LINENO = lINENO;
	}
	public String getMATNR() {
		return MATNR;
	}
	public void setMATNR(String mATNR) {
		MATNR = mATNR;
	}
	public String getMTART() {
		return MTART;
	}
	public void setMTART(String mTART) {
		MTART = mTART;
	}
	public String getMAKTX() {
		return MAKTX;
	}
	public void setMAKTX(String mAKTX) {
		MAKTX = mAKTX;
	}
	public String getMEINS() {
		return MEINS;
	}
	public void setMEINS(String mEINS) {
		MEINS = mEINS;
	}
	public String getMSEHT() {
		return MSEHT;
	}
	public void setMSEHT(String mSEHT) {
		MSEHT = mSEHT;
	}
	public String getMATKL() {
		return MATKL;
	}
	public void setMATKL(String mATKL) {
		MATKL = mATKL;
	}
	public String getWGBEZ() {
		return WGBEZ;
	}
	public void setWGBEZ(String wGBEZ) {
		WGBEZ = wGBEZ;
	}
	public String getLVORM() {
		return LVORM;
	}
	public void setLVORM(String lVORM) {
		LVORM = lVORM;
	}
	public String getWERKS() {
		return WERKS;
	}
	public void setWERKS(String wERKS) {
		WERKS = wERKS;
	}
	public String getEKGRP() {
		return EKGRP;
	}
	public void setEKGRP(String eKGRP) {
		EKGRP = eKGRP;
	}
	public String getEKNAM() {
		return EKNAM;
	}
	public void setEKNAM(String eKNAM) {
		EKNAM = eKNAM;
	}
	public String getMMSTA() {
		return MMSTA;
	}
	public void setMMSTA(String mMSTA) {
		MMSTA = mMSTA;
	}
	public String getLVORM1() {
		return LVORM1;
	}
	public void setLVORM1(String lVORM1) {
		LVORM1 = lVORM1;
	}

}
