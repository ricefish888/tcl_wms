package com.vtradex.wms.webservice.sap.model;

import com.vtradex.wms.server.model.entity.item.WmsItemHandOverAtt;
import com.vtradex.wms.server.model.entity.item.WmsItemJITAtt;
import com.vtradex.wms.server.service.annotation.XmlDataNote;

/***
 * wms 给 sap  物料属性  JIT属性以及交接属性
 * @author administrator
 *
 */
@XmlDataNote("Wms2SapItemAttr")
public class Wms2SapItemAttr {
	
	 
	/***
	 * 物料编码
	 */
	private String itemCode;
	
	/**
	 * JIT属性
	 * {@link WmsItemJITAtt}
	 * */
	private String jitAttr;
	
	/**
	 * 交接属性
	 * {@link WmsItemHandOverAtt}
	 * */
	private String handoverAttr;

	public String getItemCode() {
		return itemCode;
	}

	public void setItemCode(String itemCode) {
		this.itemCode = itemCode;
	}

	public String getJitAttr() {
		return jitAttr;
	}

	public void setJitAttr(String jitAttr) {
		this.jitAttr = jitAttr;
	}

	public String getHandoverAttr() {
		return handoverAttr;
	}

	public void setHandoverAttr(String handoverAttr) {
		this.handoverAttr = handoverAttr;
	}
	 
}
