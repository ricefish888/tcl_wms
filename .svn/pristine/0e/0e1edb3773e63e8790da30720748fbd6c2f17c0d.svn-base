<?xml version="1.0" encoding="UTF-8"?>
<!-- 盘点输入货品 -->
<RFFormPage id="tclCreateWmsBolPage3" title="tclCreateWmsBolPage3">
	<RFDisplay id="workDocItemDisplay" load="tclRfBolManager.showWorkDocInfo">
		<!-- format节点：控制页面展示域展示哪些字段及其展示格式 -->
		<format>
		<![CDATA[
		  	作业单号:{asnCode}
		    {itemInfos}
		]]>
		</format>
		<!-- help节点：罗列页面展示域可供展示的字段列表 -->
		<help>	
		   	 作业单号:{asnCode}
		    {itemInfos}
		</help>
	</RFDisplay>
	<RFForm>
		<buttons>
			<RFButton id="addWorkDocConfirmScan" title="addWorkDocConfirmScan" action="tclRfBolManager.addWorkDocConfirm" validate="" onFoucs="false"/>
		</buttons>
	</RFForm>
</RFFormPage>
