        <detail entityClass="com.vtradex.wms.server.model.entity.receiving.WmsASNDetail">
            <datasource><![CDATA[SELECT asnDetail.id,asnDetail.lineNo,warehouse.name,factory.name,deliveryOrder.code,asnDetail.item.code,asnDetail.item.name,asnDetail.packageUnit.unit,asnDetail.expectedPackQty,asnDetail.expectedQty,asnDetail.expectedUnpackQty,asnDetail.shortQty,asnDetail.receivedQty,asnDetail.movedQty,asnDetail.lotInfo.storageDate,asnDetail.lotInfo.productDate,asnDetail.lotInfo.expireDate,asnDetail.lotInfo.lot,asnDetail.lotInfo.soiCode,asnDetail.lotInfo.asnCustomerBill,asnDetail.lotInfo.supplierCode,supplier.name,asnDetail.asn.userField5,asnDetail.lotInfo.serialNo,pd.po.code,asnDetail.label,asnDetail.lotInfo.extendPropC12,asnDetail.userField1,asnDetail.userField2,asnDetail.userField3,asnDetail.inventoryStatus,asnDetail.lotInfo.extendPropC17  FROM WmsASNDetail asnDetail
             LEFT JOIN asnDetail.poDetail pd
             LEFT JOIN pd.po 
             LEFT JOIN asnDetail.asn.supplier supplier
             LEFT JOIN asnDetail.transportOrderDetail.deliveryOrderDetail.factory factory
             LEFT JOIN asnDetail.transportOrderDetail.deliveryOrderDetail.deliveryOrder deliveryOrder
             LEFT JOIN asnDetail.transportOrderDetail.deliveryOrderDetail.deliveryOrder.warehouse warehouse
		 WHERE 1=1
		 AND asnDetail.asn.id = {asn.id}
		 ORDER BY asnDetail.lineNo ASC]]></datasource>
