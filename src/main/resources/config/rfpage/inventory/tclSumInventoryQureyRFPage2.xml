<?xml version="1.0" encoding="UTF-8"?>
<!-- 库存查询页面模板 -->
<RFFormPage id="tclSumInventoryQureyRFPage2" title="tclSumInventoryQureyRFPage2">
	<RFDisplay id="inventoryQueryComfirmDisplay" load="tclRfinventoryQueryManager.inventorySumInfo">
		<!-- format节点：控制页面展示域展示哪些字段及其展示格式 -->
		<format>
		<![CDATA[
			仓库:{warehouseName}
		   {itemInfos}
		]]>
		</format>
		<!-- help节点：罗列页面展示域可供展示的字段列表 -->
		<help>	
			仓库:{warehouseName}
		    {itemInfos}
		</help>
	</RFDisplay>
	<RFForm>
		<inputs>
		</inputs>
		<buttons>
			<RFButton id="inventoryQuerySwitchReturn" title="inventoryQuerySwitchReturn" action="tclRfinventoryQueryManager.switchReturn" validate="" onFoucs="false"/>
		</buttons>
	</RFForm>
</RFFormPage>
