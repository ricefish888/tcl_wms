<?xml version="1.0" encoding="UTF-8"?>
<!-- ASN收货表单页面模板 -->
<RFFormPage id="tclMoveNPBySkuLocationPage1" title="tclMoveNPBySkuLocationPage1">
	<RFDisplay id="moveNoPlanBySkuDisplay" load="tclRfNPMoveBySkuManager.loctionInfo">
		<!-- format节点：控制页面展示域展示哪些字段及其展示格式 -->
		<format>
		<![CDATA[		
			库位:{locCode}
		]]>
		</format>
		<!-- help节点：罗列页面展示域可供展示的字段列表 -->
		<help>	
			库位:{locCode}
		</help>
	</RFDisplay>
	<RFForm>
		<inputs>
			<RFText id="moveNoPlanBySkuItemInput" title="moveNoPlanBySkuItemInput" tip=""
				 onFoucs="true">
			</RFText>
		</inputs>
		<buttons>
			<RFButton id="moveNoPlanBySkuItemScan" title="moveNoPlanBySkuItemScan" action="tclRfNPMoveBySkuManager.tclMoveBySkuItemCommit" validate="" onFoucs="false"/>
		</buttons>
	</RFForm>
</RFFormPage>
