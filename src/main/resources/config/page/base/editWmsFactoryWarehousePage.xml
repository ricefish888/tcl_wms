                </list>
                
                
                <remote id="factoryWarehouse.warehouse.id" title="factoryWarehouse.warehouse.id" row="3" col="1" span="1" readOnly="false" required="true" reserve="false" forceOverride="true" focusUI="false" inVisible="false" editModel="" language="false" multiple="false" manualEditable="true" displayColumn="3" percentSignPosition="both" displayedTableHead="序号,代码,名称">
                    <visibleExpression/>
