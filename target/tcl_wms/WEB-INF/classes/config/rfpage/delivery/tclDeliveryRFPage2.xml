<?xml version="1.0" encoding="UTF-8"?>
<!--交货输入页面模板 -->
<RFFormPage id="tclDeliveryRFPage2" title="tclDeliveryRFPage2">
	<RFDisplay id="deliveryDisplay" load="tclRfDeliveryManager.deliveryInfo">
		<!-- format节点：控制页面展示域展示哪些字段及其展示格式 -->
		<format>
		<![CDATA[
			收货人: {receiver}
		]]>
		</format>
		<!-- help节点：罗列页面展示域可供展示的字段列表 -->
		<help>
			收货人: {receiver}
		</help>
	</RFDisplay>
	<RFForm>
		<inputs>
			<RFText id="followInput" title="followInput" tip="followInput" onFoucs="false">
			</RFText>
		</inputs>
		<buttons>
			<RFButton id="bolScanBt" title="bolScanBt" action="tclRfDeliveryManager.createBol" validate="" onFoucs="false"/>
		</buttons>
	</RFForm>
</RFFormPage>