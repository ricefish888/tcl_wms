<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.vtradex.wms.server.model.entity.base.WmsImportTemplateDownload" table="IMPORT_TEMPLATE_DOWNLOAD">
        <id name="id" column="ID" type="long">
            <generator class="native">
                <param name="sequence">WSEQ_IMPORT_TEMPLATE_DOWNLOAD</param>
                <param name="parameters">START WITH 1000</param>
            </generator>
        </id>
        <property name="type" type="string" column="TYPE" length="100"/>
        <property name="code" type="string" column="CODE" length="100"/>
        <property name="name" type="string" column="name" length="100"/>
        <property name="fileFullPath" type="string" column="FILE_FULL_PATH" length="255"/>
        <property name="fileName" type="string" column="FILE_NAME" length="255"/>
        <property name="downloadUrl" column="DOWNLOAD_URL" type="string" length="255"/>
	 	<component name="updateInfo" class="com.vtradex.thorn.server.model.UpdateInfo">
			<property name="creatorId" type="long" column="CREATOR_ID"/>
			<property name="creator" type="string" column="CREATOR" length="50" />
			<property name="createdTime" type="timestamp" column="CREATED_TIME"/>
			<property name="lastOperatorId" type="long" column="LAST_OPERATOR_ID"/>
			<property name="lastOperator" type="string" column="LAST_OPERATOR" length="50" />
			<property name="updateTime" type="timestamp" column="UPDATE_TIME"/>
		</component>
	 </class>
</hibernate-mapping>