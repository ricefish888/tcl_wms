返回列表 = 创建列表()
$规则名称结果集=多值查表(组织,"R101_波次生成规则表")
如果($规则名称结果集==空){
     异常("请维护“R101_波次生成规则表”")
}

每个($规则:$规则名称结果集){
     $货主编码=$规则.货主编码
    
     $起始时间=$规则.拣货单起始时间
     如果($规则.拣货单起始时间==空){
          $起始时间="00:00:00"
     }
     $拣货单起始时间=时间转换日期($起始时间)
     $拣货单截止时间=时间转换日期($规则.拣货单截止时间)
     如果($规则.规则名称=="R101_按承运商"){
          R101_按承运商()
     }
     
}
